());\r\n    },[setDados]);\r\n\r\n    console.log(dados.responseText);\r\n    return(\r\n        <div>\r\n            <h2>Data: {dados.responseText}</h2>\r\n            <Inner fn={fn} />\r\n        </div>\r\n    );\r\n}\r\n\r\nconst Inner = React.memo(function MyComponent(props) {\r\n    console.log('%crenderizando FILHO, repare que será apenas uma vez','background-color:green;color:white;font-size:14px;padding:10px');\r\n    return <div><button onClick={props.fn}>Atualizar</button></div>\r\n});\r\n\r\n","C:\\Users\\crono\\OneDrive\\Área de Trabalho\\javascript\\Aulas\\React-js\\nav-hooks\\src\\components\\createContext.jsx",["62"],"C:\\Users\\crono\\OneDrive\\Área de Trabalho\\javascript\\Aulas\\React-js\\nav-hooks\\src\\components\\useContext.jsx",["63"],{"ruleId":"64","severity":1,"message":"65","line":4,"column":1,"nodeType":"66","endLine":11,"endColumn":2},{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","replacedBy":"70"},{"ruleId":"64","severity":1,"message":"65","line":12,"column":1,"nodeType":"66","endLine":52,"endColumn":2},{"ruleId":"64","severity":1,"message":"65","line":2,"column":1,"nodeType":"66","endLine":8,"endColumn":2},{"ruleId":"64","severity":1,"message":"65","line":3,"column":1,"nodeType":"66","endLine":20,"endColumn":2},{"ruleId":"64","severity":1,"message":"65","line":3,"column":1,"nodeType":"66","endLine":22,"endColumn":2},{"ruleId":"64","severity":1,"message":"65","line":3,"column":1,"nodeType":"66","endLine":26,"endColumn":2},{"ruleId":"64","severity":1,"message":"65","line":10,"column":1,"nodeType":"66","endLine":24,"endColumn":2},{"ruleId":"64","severity":1,"message":"65","line":10,"column":1,"nodeType":"66","endLine":21,"endColumn":2},{"ruleId":"64","severity":1,"message":"65","line":4,"column":1,"nodeType":"66","endLine":14,"endColumn":2},"import/no-anonymous-default-export","Unexpected default export of anonymous function","ExportDefaultDeclaration","no-native-reassign",["71"],"no-negated-in-lhs",["72"],"no-global-assign","no-unsafe-n